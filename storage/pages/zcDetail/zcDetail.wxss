page{font-size: 28rpx; color: #6b6b6b;}
view{overflow: visible !important;}
.borb12{border-bottom: 12rpx solid rgb(242, 242, 242);}
.metatit{font-size: 30rpx; color: #333; font-weight: 700;}
.swiper-hd{height: 640rpx;}
.swiper-hd image{width: 100%; height: 100%;}
.hd-info{padding: 0 20rpx;}
.hd-name{font-size: 30rpx; font-weight: 700; color: #333; margin: 20rpx 0;}
.hdv{display: flex; align-items: center; margin-bottom: 20rpx;}
.hd-bar{flex: 1;}
.hd-bar .hd-param-sup{float: left;}
.hd-bar .hd-bar-params{margin-bottom: 10rpx; display: flex; align-items: center; justify-content: space-between;}
.hd-param-sup .hd-sup-alr,
.hd-bar .hd-per-figure{color: #f00;}
.hd-bar .hd-sup-per{float: right;}
.hd-bar .hd-bar-box{margin-bottom: 10rpx;}
.hd-bar .hd-bar-box .wxzx-progress{width: 100%;}
.hd-bar-status{display: flex; align-items: center; justify-content: space-between;}
.hd-bar .stillTime{color: #f00;}
.hd-share{text-align: right; font-size: 0; width: 100rpx;}
.hd-share image{width: 48rpx; height: 48rpx;}
.hd-share view{font-size: 24rpx;}
.ser-promise{position: relative; padding: 0 20rpx;}
.ser-promise .metatit{position: absolute; left: 20rpx; top: 20rpx;}
.ser-promise .serList{padding: 20rpx 0 10rpx 100rpx;}
.ser-promise .serItem{margin-bottom: 10rpx; display: flex; align-items: center;}
.ser-promise .serItem image{width: 30rpx; height: 30rpx; margin-right: 16rpx; }
.ser-promise .serItem text{color: #93291e;}
.cho-spec{padding: 20rpx 0; position: relative;}
.cho-spec .metatit{position: absolute; left: 20rpx; top: 20rpx;}
.cho-spec .classify{margin-left: 120rpx; height: 40rpx; line-height: 40rpx;}
.cho-spec .classify text{display: inline-block; line-height: 40rpx;}
.cho-spec .classify image{width: 14rpx; height: 22rpx; position: absolute; right: 20rpx; top: 50%; margin-top: -11rpx;}
/* tabBox section */
.tab-hd{display: flex; align-items: flex-start; justify-content: space-around; height: 78rpx; line-height: 74rpx; border-bottom: 2rpx solid #e4e4e4; margin-bottom: 4rpx;}
.tab-hd .active{color: #f10; border-bottom: 4rpx solid #f10;}
.tab-bd{min-height: 1000rpx;}
.tab-bd swiper-item{padding: 0 20rpx;}
.ft{position: fixed; bottom: 0; left: 0; width: 100%; height: 100rpx; border-top: 1px solid #e4e4e4; background-color: #fff;}
.ftList{display: flex; text-align: center; height: 100rpx; line-height: 100rpx;}
.ftList .cell{line-height: normal; box-sizing: border-box; max-width: 100rpx; min-width: 13%; flex: 1; display: block; height: 100rpx; color: #999; padding-top: 10rpx;}
.ftList image{width: 40rpx; height: 40rpx; display: block; margin: 8rpx auto 0;}
.ftList .ft-btn{max-width: 450rpx; box-sizing: border-box; background-color: #cc363f; color: #fff; text-align: center; display: block; flex: 1;}
.fix-iphonex{bottom:68rpx!important;}
.fix-iphonex::after {content: ' '; position: fixed; bottom: 0!important; height: 68rpx!important; width: 100%; background: #fff;}
/* dialog section */
.dialog-container{position: fixed; left: 0; bottom: 0; width: 0; height: 100%; z-index: 1000;}
.dialog-container.dialog-show{width: 100%;}
.mask{display: none; position: absolute; top: 0; left: 0; right: 0; width: 100%; height: 100%; z-index: -1; background-color: rgba(0, 0, 0, .7);}
.dialog-container.dialog-show .mask{display: block;}
.dialog{display: flex; -webkit-box-orient: vertical; -webkit-box-direction: normal; flex-direction: column; position: absolute; bottom: 0; left: 0; transition: transform .3s; background-color: #fff; color: #333; font-size: 28rpx; width: 100%; height: 83%; transform: translateY(100%); -webkit-transform: translateY(100%);}
.dialog.dialog-popup{transform: translateY(0); -webkit-transform: translateY(0);}
.dialog.dialog-popup .sku-img{display: block;}
.dialog-content{display: flex; flex: 1; padding: 0 20rpx; overflow: auto;}
.dialog-xsku{width: 100%;}
.tb-sku-container{display: flex; width: 100%; height: 100%; -webkit-box-orient: vertical; -webkit-box-direction: normal; flex-direction: column; font-size: 28rpx; color: #333; box-sizing: border-box; padding: 20rpx 0;}
.sku-pro{position: relative; padding-bottom: 32rpx; border-bottom: 2rpx solid #f2f2f2;}
.sku-pro .sku-img{position: absolute; left: 20rpx; top: -120rpx; padding: 10rpx; background-color: #fff; box-sizing: border-box; width: 240rpx; height: 240rpx; box-shadow: 2rpx 2rpx 4rpx 4rpx rgba(0,0,0,.3); display: none;}
.sku-pro .sku-img image{width: 100%; height: 100%;}
.sku-pro-info{margin: 20rpx 0 0 300rpx;}
.sku-pro-info .h{font-size: 40rpx; color: #ff2d00;}
.sku-pro-info .sku-txt{display: flex; justify-content: space-between; align-items: center; color: #949494;}
.sku-pro-info .sku-txt text{color: #93291E;}
.sku-close{position: absolute; right: 0; top: 0;}
.sku-info{overflow: auto !important;}
.sku-info .sku-spec,
.sku-story{padding-bottom: 32rpx; border-bottom: 2rpx solid #f2f2f2;}
.sku-info .sku-tit{font-size: 32rpx; color: #111; padding: 20rpx 0;}
.sku-info .sku-specs text{padding: 10rpx 60rpx; background-color: #e4e4e4; color: #363636; border-radius: 8rpx; margin: 0 20rpx 20rpx 0; display: inline-block;}
.sku-info .sku-specs text.active{background-color: #f00; color: #fff;}
.quantity-info{border-bottom: 2rpx solid #f2f2f2;}
.sku-quantity{display: flex; align-items: center; padding: 20rpx 0;}
.sku-quantity .sku-tit{flex: 2;}
.sku-quantity .btn-minus,
.sku-quantity .btn-input,
.sku-quantity .btn-plus{width: 70rpx; height: 70rpx; text-align: center; line-height: 70rpx; border: 2rpx solid #e4e4e4;}
.sku-quantity .btn-minus{border-right: 0; border-top-left-radius: 8rpx; border-bottom-left-radius: 8rpx;}
.sku-quantity .btn-plus{border-left: 0; border-top-right-radius: 8rpx; border-bottom-right-radius: 8rpx;}
.sku-quantity .btn-input input{width: 100%; height: 100%;}
.gears{display: flex; flex-wrap: wrap;}
.gears .gear{width: 180rpx; border: 2rpx solid #e4e4e4; line-height: 80rpx; border-radius: 8rpx; margin-right: 20rpx; text-align: center;}
.gears .gearAll{line-height: normal;}
.gears .gear .gearMon{color: #999;}
.sku-btns{padding-top: 34rpx;}
.sku-btns .support{height: 80rpx; text-align: center; line-height: 80rpx; color: #fff; background-color: #ff6700;}


