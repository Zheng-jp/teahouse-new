/* storage/pages/out_of_warehouse/out_of_warehouse.wxss */
/* view{overflow: initial;} */
page{background-color: #fff;}
.wrapper{margin-bottom: 50px;}
.header{display: flex; align-items: center; justify-content: space-around; padding: 20rpx;}
.header .tea-img-box{width: 40px; height: 40px; overflow: hidden;}
.header .tea-img-box image{width: 100%; height: 100%;}
.header .tea-name{font-size: 28rpx; color: #797979; font-weight: 700; max-width: 130rpx; white-space: nowrap; text-overflow: ellipsis; overflow: hidden;}
.header .tea-date{font-size: 22rpx; color: #333; letter-spacing: 0;}
.header .stock-addr{font-size: 20rpx; color: #999900; border-radius: 6rpx; border: 2rpx solid #999900; padding: 4rpx 10rpx; text-align: center; display: inline-block;}
.real-time-stock{width: 430rpx; height: 110rpx; border: 2rpx solid #821006; border-radius: 6rpx; overflow: hidden;}
.real-time-stock .real-time-title{background-color: #821006; font-size: 24rpx; color: #fff; text-align: center; padding: 8rpx 0;}
.real-time-stock .stock-box{font-size: 24rpx; color: #811006; text-align: center; overflow: hidden; display: flex; align-items: center; justify-content: flex-end; height: 66rpx;}
.real-time-stock .stock-num{display: inline-block; width: 90rpx; margin: 0 4rpx; overflow-x: auto; white-space: nowrap; color: #fff; font-size: 26rpx; background-color: #811006; border-radius: 6rpx;}
.icon-more{width: 14rpx; height: 24rpx; margin-left: 10rpx;}
.stock-container{display: flex; align-items: center;}
.list-item{display: flex; align-items: center; justify-content: space-between; padding: 20rpx;}
.list-item .list-key{font-size: 32rpx; font-weight: 700; color: #00a0e6;}
.list-item .list-key .explain{display: inline-block; width: 30rpx; height: 30rpx; text-align: center; line-height: 30rpx; border-radius: 50%; background-color: #bbb; color: #fff; margin-left: 6rpx; font-size: 24rpx;}
.list-item .list-value{font-size: 32rpx; color: #333; position: relative;}
.list-item .list-value.price{color: #CD300E;}
.list-item .list-value radio{vertical-align: middle;}
.list-item .list-value input{text-align: right;}
.btn{background-color: #ff6700; color: #fff; width: 95%;}
.agreement{font-size: 28rpx; margin: 20rpx 0 40rpx; padding: 0 20rpx;}
.agreement checkbox{box-sizing: border-box;}
.agreement text{color: #00A0E6; font-weight: 700;}
.list-item.outNum .list-value{display: flex; align-items: center; color: #811006; font-weight: 700;}
.list-item.outNum .list-value input{width: 60px; border: 1px solid #811006; border-radius: 3px; box-sizing: border-box; 
text-align: center; font-weight: normal;}
.list-item.outNum .list-value button{font-size: 10px; color: #fff; background-color: #821006;}
.list-item.freightCost .list-value text{color: #ff6700;}
.list-item.mailing .list-value{color: #999; text-align: right; padding-right: 30rpx;}
.list-item.mailing view{flex: 1;}
.list-item.mailing .address .icon-more{position: absolute; right: 0; top: 50%; margin-top: -5px;}
.ft{position: fixed; bottom: 0; left: 0; width: 100%; height: 50px; background-color: #ebebeb; display: flex; align-items: center; justify-content: space-between;}
.ft .outCost{padding-left: 10px; color: #000;}
.ft .outCost text{color: #811006;}
.ft .outBtn{background-color: #811006; color: #fff; padding: 0 15px; line-height: 50px;}


/* 支付密码css start */
.bg_layer{position: fixed; left: 0; top: 0; bottom: 0; right: 0; background-color: rgba(0, 0, 0, 0.6); z-index: 9998; }
.input_main{position: fixed; left: 0; bottom: 500rpx; width: 100%; height: 394rpx;background-color: #fff; z-index: 9999;}
.input_title{width: 100%; height: 90rpx; line-height: 90rpx; text-align: center; font-size: 32rpx; border-bottom: 1rpx solid #e2e2e2;}
.input_back{position: absolute; left: 0; top: 0;width: 80rpx; height: 90rpx; display: flex; justify-content: center; align-items: center;}
.input_back text{width: 20rpx;height: 20rpx;background-color: white;border: 1rpx solid #aaa;border-width: 5rpx 0 0 5rpx;transform: rotate(-45deg);}
.input_tip{margin: 30rpx; font-size: 24rpx; color: #888; }
/* 密码掩码模拟 */
.input_row{width: 690rpx; margin: 0 auto; height: 98rpx; position: relative;display: flex; align-items: center; border: 1rpx solid #e2e2e2; border-radius: 20rpx;}
.input_row .pwd_item{flex: 1; display: flex; align-items: center; justify-content: center; height: 100%; border-right: 1rpx solid #e2e2e2; position: relative;}
.pwd_item:nth-last-of-type(1) { border-right: 0; } 
.pwd_item text { width: 30rpx; height: 30rpx; border-radius: 30rpx; background-color: #555;}
.forget_pwd{ float: right; margin: 30rpx; width: 100rpx; text-align: right; font-size: 24rpx; color: #ff7800;}
/* 文本输入框位置: 设置到左边隐藏 */
.input_control { position: relative; left: -300rpx; bottom: 0; width: 100rpx; height: 100rpx;}
/* 支付密码css end */


/* 支付start */
.pm-content{
    position: absolute;
    left: 50%;
    bottom: 10px;
    width: 86%;
    transform: translateX(-50%);
  }
  .pm-content .pm-account,
  .pm-content .pm-wxpay{
    width: 100%;
    background-color: #fff;
  }
  .pm-content .pm-account{
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
  }
  .pm-content .pm-wxpay{
    border-top: 1px solid #dedede;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
  }
  .pm-content .pm-inner-view{
    position: relative;
    min-height: 60px;
  }
  .pm-content .pm-inner-view .pm-o{
    text-align: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
    height: 60px;
  }
  .pm-content .pm-inner-view .pm-o{
    font-size: 18px;
    color: #0099ff;
  }
  .pm-content .pm-inner-view .pm-o .pm-y{
    font-size: 12px;
    color: #868686;
  }
  .pm-content image{
    position: absolute;
    left: 50px;
    top: 50%;
    margin-top: -15px;
    width: 30px;
    height: 30px;
    margin-right: 30px;
  }
  .pm-content .cancel{
    background-color: #fff;
    font-size: 16px;
    color: #0099ff;
    text-align: center;
    min-height: 40px;
    line-height: 40px;
    border-radius: 5px;
    margin-top: 10px;
  }
  .mask{
    position: absolute;
    top: 0;
    left: 0; 
    right: 0;
    width: 100%;
    height: 100vh;
    background-color: rgba(0, 0, 0, .5);
  }
  /* 支付end */
  