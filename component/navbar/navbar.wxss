/* component/navbar/navbar.wxss */
.custom{
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    height: 45px;
    background: rgb(249,249,249);
    z-index: 999;
  }
  
  .title-container {
    height: 44px;
    display: flex;
    align-items: center;
    position: relative;
  }
  
  .capsule {
    margin-left: 10px;
    height: 30px;
    /* border: 1px solid #fff; */
    border: 1px solid rgb(212,212,212);
    border-radius: 16px;
    display: flex;
    align-items: center;
  }
  
  .capsule > view {
    width: 40px;
    height: 60%;
  
    position: relative;
  }
  .capsule > view:nth-child(2) {
    /* border-left: 1px solid #fff; */
    border-left: 1px solid rgb(212,212,212);
  }
  
  .capsule image {
    width: 50%;
    height: 100%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
  }
  
  .title {
    /* color: white; */
    color: black;
    position: absolute;
    left: 104px;
    right: 104px;
    font-size: 14px;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
  }