<view>
    <view class="list" bindtap="go_change_account">
        <view class="tilte_text">当前账号</view>
        <view class="content">
            <view class="context">{{user_phone}}</view>
            <image src="http://zhihuichacang.com/u1959.png" mode="widthFix" class="more" />
        </view>
    </view>
    <view class="list" bindtap="go_change_name">
        <view class="tilte_text">昵称</view>
        <view class="content">
            <view class="context">{{user_name}}</view>
            <image src="http://zhihuichacang.com/u1959.png" mode="widthFix" class="more" />
        </view>
    </view>
    <view class="list">
        <view class="tilte_text">我的邀请码</view>
        <view class="content">
            <view class="context">{{dimension}}</view>
        </view>
    </view>
    <view class="list">
        <view class="tilte_text">朋友邀请码</view>
        <view class="content">{{dimension_up}}</view>
    </view>
    <view class="submit">保存</view>
</view>