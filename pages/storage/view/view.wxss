/* pages/storage/view/view.wxss */
view{overflow: initial !important;}
page{height: 100%;}
swiper.swiper-container{height: 93%;}
.mask{position: fixed; top: 0; left: 0; width: 100%; height: 100%; background-color: rgba(0, 0, 0, .5);}
.overflowYauto{overflow-y: auto !important;}
/* .overflowYauto scroll-view{margin-bottom: 50px;} */
.hid-txt-two{overflow: hidden !important; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;}
.hid-txt-one{overflow: hidden !important; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 1; -webkit-box-orient: vertical;}
.swiper-tab{display: flex; width: 100%;border-bottom: 2rpx solid #ccc;text-align: center;height: 88rpx;line-height: 88rpx;}
.swiper-tab-item{flex: 1; color: #bcbcbc;}
.active{color:#93291f; position: relative;}
.active::after{content: ''; width: 40%; position: absolute; bottom: 0; left: 50%; margin-left: -20%; display: block; border: 2rpx solid #93291f;}

.gross-container{position: relative; height: 100rpx; display: flex; justify-content: space-around; align-items: center; background-color: #f9f9f9;}
.gross-container .gross-item{text-align: center;}
.gross-container .gross-title{font-size: 32rpx; color: #000;}
.gross-container .gross-number{font-size: 48rpx; color: #cd300e;}
.gross-container .choice-warehouse{font-size: 24rpx; color: #bcbcbc; padding-left: 40rpx; border-left: 2rpx solid #bcbcbc;}
.icon-choice{float: right; width: 25rpx; height: 14rpx; margin: 10rpx 0 0 10rpx;}
.scaley-img{transform: scaleY(-1);}
.warehouse-container{position: absolute; top: 100%; width: 100%; background-color: #f9f9f9; z-index: 999;}
.item-box{display: flex; padding: 40rpx 20rpx 0; font-size: 32rpx;}
.warehouse-left{flex-basis: 20%; text-align: center; align-items: center; padding-top: 8rpx;}
.warehouse-left text{color: #93291f; font-weight: 700;}
.warehouse-right{flex-basis: 80%; display: flex; align-items: center; flex-wrap: wrap;}
.warehouse-right .warehouse-item{color: #93291f; background-color: #fff; text-align: center; padding: 8rpx 20rpx; margin: 0 10rpx 20rpx 0; box-sizing: border-box;}
.retract{text-align: center; background-color: #d7d7d7;}
.retract .icon-retract{width: 50rpx; height: 50rpx;}
.storage-enviroment{border: 1px solid #d7d7d7; box-sizing: border-box; height: 60rpx; display: flex; align-items: center;}
.storage-enviroment .storage-name-box{display: flex; align-items: center; flex-basis: 25%; height: 100%;}
.storage-enviroment .storage-name-box image{width: 40rpx; height: 40rpx; margin: 0 10rpx;}
.storage-enviroment .storage-name-box .storage-name{color: #999900; font-size: 28rpx; white-space: nowrap;}
.enviroment-data-box{flex: 75%; height: 100%; background-color: #d7d7d7; display: flex; align-items: center; justify-content: space-between; padding-right: 20rpx;}
.enviroment-data-box .icon-temperature,
.enviroment-data-box .icon-humidity,
.enviroment-data-box .icon-video{width: 40rpx; height: 40rpx;}
.enviroment-data-box .enviroment-item{display: flex; align-items: center; height: 100%;}
.enviroment-data-box .item-video{background-color: #e4e4e4; padding: 0 14rpx;}
.enviroment-item .temperature{font-size: 28rpx; color:#00BBFF;}
.enviroment-item .humidity{font-size: 28rpx; color: #66CC00;}
.more image{width: 28rpx; height: 30rpx;margin-top: 4px;}

.storage-item{padding: 40rpx 10rpx; border-bottom: 4rpx solid #f9f9f9; display: flex;}
.item-left{text-align: center; flex-basis: 35%; display: flex; flex-direction: column; align-items: center; justify-content: space-between;}
.item-left .display-img{width: 120rpx; height: 120rpx;}
.item-left .tea-name{font-size: 36rpx; font-weight: bold; color: #797979;}
.item-left .tea-date{font-size: 24rpx; color: #333;}
.item-left .tag-box{display: inline-flex; align-items: flex-end;}
.item-left .tag-box .tag-text{font-size: 24rpx; color: #999900; border: 1px solid #999900; padding: 4rpx 0; border-radius: 8rpx; width: 60px; box-sizing: border-box; overflow: hidden; white-space: nowrap;}
.item-left .tag-box .tag-text:first-of-type{margin-right: 8rpx;}
.item-right{flex-basis: 65%;}
.tea-stock{font-size: 24rpx; color: #868686; font-weight: 700; display: flex; align-items: center; justify-content: space-around;}
.tea-stock .stock-title{font-size: 24rpx; font-weight: 700; color: #000;}
.tea-stock .stock-num{font-size: 44rpx; font-weight: 700; color: #93291E;}
.tea-stock .stock-more{font-size: 24rpx; color: #0099FF;}
.exchange-price{background-color: #f2f2f2; margin-bottom: 20rpx;}
.exchange-price text{font-size: 24rpx; color: #6b6b6b;}
.exchange-price text.out-price{color: #D81E06;}
.exchange-price>view{display: flex; justify-content: space-between; align-items: center; margin-bottom: 16rpx;}
.btns{display: flex; justify-content: space-between; font-size: 32rpx;}
.btns button{ height: 70rpx; line-height: 70rpx;}
.btns .out-storage{background-color: #00bbff; flex: 1; color: #fff;}
.btns .renew{flex-basis: 30%; margin-left: 5px;}
/* 众筹 */
.swiper-box{background-color: #000; box-sizing: border-box; height: 400rpx;}
.swiper-box swiper{height: 160px;}
.swiper-title{text-align: center; color: #fff; font-size: 32rpx; padding: 20rpx 0;}
.zc-swiper swiper-item{text-align: center; height: initial !important;}
.zc-swiper swiper-item .box{width: 90%; height: 250rpx; background-color: #fff; display: inline-block; box-sizing: border-box; padding: 36rpx 12rpx; display: inline-flex; justify-content: space-between;}
.zc-swiper .wx-swiper-dot{width: 40rpx; display: inline-flex; height: 10rpx; margin-left: 20rpx; justify-content: space-between;}
.zc-swiper .wx-swiper-dot::before{content: ''; background-color: #757575; flex-grow: 1; border-radius: 8rpx;}
.zc-swiper .wx-swiper-dot-active::before{background-color: #fff;}
.box .swiper-img-box{width: 180rpx; height: 180rpx; margin-right: 20rpx;}
.box .swiper-img-box image{width: 100%; height: 100%;}
.box .swiper-good-info{flex: 1;}
.box .swiper-good-info .swiper-stock{text-align: right; font-size: 24rpx; color: #333; margin-bottom: 10rpx;}
.box .swiper-good-info .swiper-desc{font-size: 26rpx; color: #949494; text-align: left; margin-bottom: 10rpx; height: 36px; word-break: break-all; text-overflow: ellipsis; overflow: hidden;}
.box .swiper-good-info .swiper-button{text-align: right;}
.swiper-good-info .swiper-button button{display: inline-block; width: 150rpx; height: 50rpx; background-color: #d53106; color: #fff; font-size: 28rpx; border-radius: 0; box-sizing: border-box; line-height: 50rpx;}
.switch-project .switch-btn{width: 300rpx; height: 50rpx; margin: 30rpx auto; border: 2rpx solid #868686; text-align: center; line-height: 40rpx; border-radius: 30rpx; box-sizing: border-box;}
.switch-project .switch-btn image{width: 32rpx; height: 32rpx; vertical-align: middle; margin-right: 30rpx;}
.switch-project .switch-btn text{font-size: 26rpx; color: #868686;}
.switch-project .switch-now{border-color: #D53109}
.switch-project .switch-now text{color: #D53109;}
.project-title{padding: 0 20rpx; font-size: 36rpx; font-weight: 700;}
.zc-item-box{padding: 0 20rpx;}
.zc-item{display: flex; justify-content: space-between; padding: 30rpx 0; border-bottom: 1px solid #eee;}
.zc-item .left-img{width: 230rpx; height: 230rpx; margin-right: 20rpx;}
.zc-item .left-img image{width: 100%; height: 100%;}
.zc-item .right-project-info{flex: 1;}
.wxzx-progress{display: inline-block; margin-right: 16rpx;}
.percent-num{font-size: 30rpx; color: #aeaeae; vertical-align: middle;}
.right-project-info .project-name{font-size: 30rpx; color: #333; line-height: 1em;}
.right-project-info .project-people{font-size: 26rpx; color: #aeaeae; margin: 20rpx 0 40rpx;}
.right-project-info .project-price text{font-size: 36rpx; color: #b50000;}
.right-project-info .project-price button{width: 150rpx; height: 50rpx; text-align: center; box-sizing: border-box; font-size: 26rpx; float: right; line-height: 50rpx; padding: 0; border-radius: 0;}
.right-project-info .project-price .join-project{background-color: #D53109; color: #fff;}
.right-project-info .project-price .check-project{border: 1px solid #D53109; color: #D53109; background-color: transparent;}

/* 续费弹窗 */
.renew-pop{position: absolute; top: 40%; left: 50%; width: 90%; min-height: 200px; background-color: #fff; border-radius: 8px; transform: translate(-50%, -50%);}
.renew-pop .renew-close{position: absolute; bottom: -50px; left: 50%; width: 30px; height: 30px; line-height: 22px; border: 2px solid #fff; color: #fff; border-radius: 50%;font-size: 28px; box-sizing: border-box; text-align: center; transform: translateX(-50%);}
.bd{padding: 0 30px;}
.bd .title{text-align: center; font-size: 20px; padding: 15px 0;}
.bd{ font-size: 16px;}
.bd .time-view .subtit-text{color: #00a0e6; font-weight: 700;}
.bd .time-view .time-text{color: #949494;}
.bd .renew-time .subtit-text{color: #821006; font-weight: 700;}
.bd .renew-time .time-text{color: #821006;}
.bd .time-view, .bd .renew-time{display: flex; align-items: center; justify-content: space-between; padding: 5px 0;}
.bd .num-view{display: flex; align-items: center;}
.bd .num-view text{display: inline-block; width: 35px; height: 30px; line-height: 28px; border: 1px solid #ccc; box-sizing: border-box; text-align: center;}
.bd .num-view input{border: 1px solid #ccc; border-left: 0; border-right: 0; width: 40px; height: 30px; box-sizing: border-box; text-align: center;}
.bd .num-view .minus{border-top-left-radius: 3px; border-bottom-left-radius: 3px;}
.bd .num-view .plus{border-top-right-radius: 3px; border-bottom-right-radius: 3px;}
.fd{background-color: #ebebeb; height: 50px; margin-top: 15px; display: flex; align-items: center; justify-content: space-between; border-bottom-left-radius: 8px; border-bottom-right-radius: 8px; overflow: hidden !important;}
.fd .renew-cost{padding-left: 20px; color: #111;}
.fd .renew-cost text{color: #821006;}
.fd .renew-btn{background-color: #821006; text-align: center; color: #fff; line-height: 50px; padding: 0 10px;}

