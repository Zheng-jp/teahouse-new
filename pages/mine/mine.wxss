/* my.wxss */
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #ff5
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}
.information{
  display: flex;
  height: 360rpx;
  background: #93291E;
}
.username{
  font-size: 30rpx;
}
.head_portrait{
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 0 auto;
}
.information{
  color: #fff;
}
.phone_num{
  font-size: 26rpx;
}
.information .list{
  flex: 1;
  text-align: center;
}