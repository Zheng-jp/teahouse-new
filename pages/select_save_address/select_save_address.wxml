<!--pages/select_address/select_address.wxml-->
<view class='all'>
   <radio-group class="radio-group" bindchange="radioChange">

      <view class='content_list'>
         <view class='radio'>
            <radio value="" color='#fff' />
         </view>
         <view class='add_information'>
            <view class="list1">
               <view> 仓储编号:BJ02 </view>
               <view>
                  <view class="sava_contents">
                     <view class="savetitle">仓储费标准:</view>
                     <view class='scroll_view_border'>
                        <swiper class="swiper_container" vertical="true" autoplay="true" circular="true"
                           interval="5000">
                           <block wx:for="{{noticeList}}">
                              <swiper-item>
                                 <view class="swiper_item"> ¥{{item.content0}}/天 </view>
                              </swiper-item>
                           </block>

                        </swiper>
                     </view>

                  </view>
                  <view class="tips">*出仓或换手时收取</view>

               </view>
            </view>
            <view class='address'>
               仓储地址:北京市朝阳区天鹅湾小区5号楼3单元201
            </view>
            <view class="list2">
               <view class="list2_title">
                  仓库描述：
               </view>
               <view class="list2_content">
                  云南仓库四季如春，便于普洱茶的发酵
               </view>
            </view>
         </view>
      </view>
   </radio-group>

   <!--<view class='add_address' bindtap='add_address'>新增收货地址</view>-->
</view>