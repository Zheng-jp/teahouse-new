/* pages/members/members.wxss */
.all{
  background: #F9F9F9;
}
.card{
  padding: 30rpx;
  width: 98%;
  height: 320rpx;
  background: #CCCC00;
  margin: 0 auto;
  border-radius: 20rpx;
box-sizing: border-box
}
image{
  width: 100%;
  height: 100%;

}
.card_img{
   width: 80rpx;
   height: 80rpx;
}
.img_cir{
  width: 88rpx;
  text-align: center;
  line-height: 80rpx;
  height: 88rpx;
  background: #fff;
  border-radius:50%;
  overflow: hidden; 
}
.code{
  width: 110rpx;
  height: 110rpx;
}
.card_a{
  display: flex;
  justify-content: space-between;
}
.user_infor{
  display: flex;
  color: #fff;
}
.use_num{
  margin-right: 40rpx;
  text-align: right;
  font-size: 30rpx;
  
}
.name{
  color: #fff;
  font-size: 50rpx;
  padding-top: 10rpx;
  font-weight: bold;


}
.card_c{
  color: #fff;
  display: flex;
  font-size:26rpx; 
  margin-top: 30rpx;
  justify-content: space-between;
}
.level{
  background: #fff;
  margin-top: 20rpx;
}
.level_title{
  width: 94%;
  margin:0 auto; 
    display: flex;
  flex-basis:25%;
  height: 80rpx;
  align-items: center;
  border-bottom:solid 4px #F2F2F2; 
}
.level_title view{
  font-size: 30rpx;
  font-weight:bold; 
  flex-basis:25%;
  text-align: center;
}
.level_content{
   width: 94%;
  margin:0 auto; 
}
.content_list{
  display: flex;
  height: 80rpx;
align-items: center;
 border-bottom:solid 1px #E5E5E5; 


}
.content_list>view{
 flex-basis:25%;
 text-align: center;
 font-size:26rpx; 
 font-weight: bold;



}
.level_img{
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
}
.radio_text{
  margin-left: 20rpx;
  vertical-align: -6rpx;
  color: #FF9975;
}